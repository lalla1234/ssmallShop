/**app.wxss**/

.container {
  box-sizing: border-box;
  background: -webkit-linear-gradient(left,#f3f3f3,#f4f4f4,#f5f5f5,#f6f6f6,#f7f7f7) no-repeat;
  font-family: PingFangSC-Light, helvetica, 'Heiti SC';
}

view, image, text, navigator {
  box-sizing: border-box;
  padding: 0;
  margin: 0;
}

view, text {
  font-family: PingFangSC-Light, helvetica, 'Heiti SC';
  font-size: 29rpx;
  color: #333;
}
